{
  "name": "ea-gaming-engine",
  "version": "1.0.0",
  "description": "Transform LearnDash courses into interactive educational games",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "build:games": "cd assets/games && npm run build",
    "dev:games": "cd assets/games && npm run dev",
    "lint": "eslint assets/js assets/games/src",
    "lint:fix": "eslint assets/js assets/games/src --fix",
    "i18n:make-pot": "wp i18n make-pot . languages/ea-gaming-engine.pot --domain=ea-gaming-engine --exclude=node_modules,vendor,assets/dist",
    "i18n:make-json": "wp i18n make-json languages --no-purge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easily-amused/ea-gaming-engine.git"
  },
  "author": "HonorsWP",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://honorswp.com/support/"
  },
  "homepage": "https://honorswp.com/ea-gaming-engine/",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "mini-css-extract-plugin": "^2.7.0",
    "prettier": "^3.0.0",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.2.0",
    "typescript-eslint": "^8.39.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.29.0",
    "@wordpress/block-editor": "^15.2.0",
    "@wordpress/blocks": "^15.1.0",
    "@wordpress/components": "^30.1.0",
    "@wordpress/element": "^6.28.0",
    "@wordpress/i18n": "^6.1.0",
    "phaser": "^3.70.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}